function photos(IDmp4) {
  var output = "<object id=\"player\" width=\"850\" height=\"475\" classid=\"clsid:d27cdb6e-ae6d-11cf-96b8-444553540000\" >";
  output += "<param name=\"movie\" value=\"https://sites.google.com/site/animexgl/players/player.swf\" >";
  output += "</param>";
  output += "<param name=\"allowFullScreen\" value=\"true\" >";
  output += "</param>";
  output += "<param name=\"allowscriptaccess\" value=\"always\">";
  output += "</param>";
  output += "<embed flashvars=\"file=\http://llphotos.twitvid.com/twitvidvideosv2/1/" + IDmp4 + ".mp4&provider=video&stretching=exactfit&controlbar.position=over\" src=\"https://sites.google.com/site/animexgl/players/player.swf\"  type=\"application/x-shockwave-flash\" allowscriptaccess=\"always\" allowfullscreen=\"true\" width=\"670\" height=\"400\">";
  output += "</embed>";
  output += "</object>";
  document.write (output);
}

